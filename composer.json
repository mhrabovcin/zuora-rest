    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "pdepend/pdepend": "~1.0",
        "phpmd/phpmd": "~1.0",
        "satooshi/php-coveralls": "*",
        "sebastian/phpcpd": "~2.0",
        "phpunit/phpunit": "^7.5",
